ratings=../dataset/FilmTrust/trainset.txt
ratings.setup=-columns 0 1 2
recommender=SVD
evaluation.setup=-testSet ../dataset/FilmTrust/testset.txt -tf
item.ranking=off -topN 10
num.factors=10
num.max.iter=100
batch_size=5000
learnRate=-init 0.005 -max 1
reg.lambda=-u 0.01 -i 0.02 -b 0.02 -s 0.1
output.setup=on -dir ../results/